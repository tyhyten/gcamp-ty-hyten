<div class="container">
  <div class="page-header">

  <h1>New User</h1>

  </div>

    <%= form_for @user do |f| %>
    <p><%= f.label :first_name %></p>
    <p><%= f.text_field :first_name %></p>
    <p><%= f.label :last_name %></p>
    <p><%= f.text_field :last_name %></p>
    <p><%= f.label :email %></p>
    <p class="email-field"><%= f.text_field :email %></p>

    <%= f.submit %>

    <% end %>

</div>
