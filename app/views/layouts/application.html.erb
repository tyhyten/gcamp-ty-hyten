<!DOCTYPE html>
<html>
<head>
  <title>Gcamp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<body name = "top">

    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">gCamp</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">

        </div><!--/.navbar-collapse -->
      </div>
    </nav>

    <!-- Main jumbotron for a primary marketing message or call to action -->




<%= yield %>

<div class="container">
  <footer>
   <div class="row">
     <div class="col-md-6">
       <%= link_to "About", about_path %>
       |
       <%= link_to "Terms", terms_path %>
       |
       <%= link_to "FAQ", faq_path %>
       |
       <%= link_to "Tasks", tasks_path %>
     </div>

       <div class="col-md-6 text-right">

         <%= link_to "http://www.linkedin.com" do %>
         <%= image_tag "linkedin.jpg", class: "img-footer"%>
           <% end %>

         <%= link_to "http://github.com" do %>
         <%= image_tag "githublogo.png", class: "img-footer"%>
          <% end %>
         <!-- <a href="https://github.com/">
         <%= image_tag"githublogo.png", class: "img-footer" %>
         </a> -->
         <%= link_to "http://twitter.com" do %>
         <%= image_tag "twitterlogo.png", class: "img-footer"%>
         <% end %>
         <!-- <a href="https://twitter.com/">
         <%= image_tag"twitterlogo.png", class: "img-footer" %>
         </a> -->
         <%= link_to "https://students.gschool.it/" do %>
         <%= image_tag "galvanizelogo.png", class: "img-footer"%>
         <% end %>
         <!-- <a href="https://students.gschool.it/">
         <%= image_tag"galvanizelogo.png", class: "img-footer" %>
         </a> -->
       </div>
     </div>
   </footer>
</div>

</body>
</html>
